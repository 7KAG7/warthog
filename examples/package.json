{
  "name": "example1",
  "version": "0.0.0",
  "scripts": {
    "bootstrap": "yarn",
    "db:seed:dev": "dotenv -- ts-node ../tools/seedLocal.ts",
    "generate:types": "ts-node ../tools/generateGQL.ts",
    "playground:open": "open http://localhost:$(dotenv -p APP_PORT)/playground",
    "start": "yarn generate:types && npm-run-all --parallel start:ts playground:open",
    "start:debug": "yarn generate:types && yarn start:ts --inspect",
    "start:ts": "ts-node --type-check src/index.ts",
    "test": "dotenv -- jest",
    "watch:ts": "yarn generate:types && nodemon -e ts,graphql -x ts-node --type-check src/index.ts"
  },
  "dependencies": {
    "@types/debug": "^0.0.31",
    "@types/faker": "^4.1.4",
    "@types/graphql": "^14.0.4",
    "@types/graphql-iso-date": "^3.3.0",
    "@types/isomorphic-fetch": "^0.0.34",
    "@types/jest": "^23.3.11",
    "@types/lodash": "^4.14.108",
    "@types/node-emoji": "^1.8.0",
    "@types/pg": "^7.4.8",
    "@types/prettier": "^1.15.2",
    "class-transformer": "^0.1.9",
    "class-validator": "^0.9.1",
    "dataloader": "^1.4.0",
    "debug": "^4.1.1",
    "dotenv": "^6.0.0",
    "dotenvi": "^0.5.1",
    "faker": "^4.1.0",
    "graphql": "^14.0.2",
    "graphql-binding": "^2.3.8",
    "graphql-iso-date": "^3.5.0",
    "graphql-yoga": "^1.16.9",
    "lodash": "^4.17.10",
    "node-emoji": "^1.8.1",
    "pg": "^7.3.0",
    "prettier": "^1.15.3",
    "reflect-metadata": "^0.1.12",
    "ts-node": "^7.0.0",
    "type-graphql": "^0.16.0",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.11",
    "typeorm-typedi-extensions": "^0.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^4.0.3",
    "@types/node": "^10.12.18",
    "@types/ws": "^5.1.2",
    "dotenv-cli": "^1.4.0",
    "graphql-schema-linter": "^0.1.6",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "ts-jest": "^23.10.5",
    "ts-lint": "^4.5.1",
    "tslint": "^5.11.0",
    "typescript": "3.0.1",
    "typestrict": "^0.0.9"
  },
  "resolutions": {
    "@types/graphql": "^14",
    "graphql": "^14",
    "graphql-yoga/**/graphql": "^14",
    "graphql-yoga/**/@types/graphql": "^14"
  }
}
